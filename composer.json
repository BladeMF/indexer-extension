{
    "name": "phpactor/workspace-query",
    "description": "Query a workspace",
    "type": "phpactor-extension",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "phpactor/worse-reflection": "^0.3",
        "phpactor/source-code-filesystem": "^0.1",
        "phpactor/name-specification": "^0.1",
        "thecodingmachine/safe": "^1.0",
        "dantleech/invoke": "^1.0",
        "phpactor/reference-finder": "^0.1.2",
        "phpactor/reference-finder-extension": "^0.1.3",
        "phpactor/text-document": "^1.1.3",
        "phpactor/rpc-extension": "^0.2.0@dev",
        "phpactor/worse-reflection-extension": "^0.2.0",
        "phpactor/source-code-filesystem-extension": "^0.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "friendsofphp/php-cs-fixer": "^2.15.0",
        "phpstan/phpstan": "^0.12.0",
        "phpactor/test-utils": "^1.0.2",
        "phpactor/console-extension": "^0.1.2"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\WorkspaceQuery\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\WorkspaceQuery\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\WorkspaceQuery\\Extension\\WorkspaceQueryExtension",
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "scripts": {
        "integrate": [
            "vendor/bin/phpunit",
            "vendor/bin/php-cs-fixer fix",
            "vendor/bin/phpstan analyse"
        ]
    }
}
